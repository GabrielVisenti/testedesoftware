#include <stdio.h>

int mdc(int a, int b) {
    if (a < 0) a = -a;
    if (b < 0) b = -b;
    if (a == 0 && b == 0) return 0;
    while (b != 0) {
        int temp = b;
        b = a % b;
        a = temp;
    }
    return a;
}

int main() {
    int x, y;
    printf("Digite dois numeros: ");
    scanf("%d %d", &x, &y);
    printf("MDC(%d,%d) = %d\n", x, y, mdc(x, y));
    return 0;
}
