#include <stdio.h>

const char* situacaoAluno(float n1, float n2, float n3, float n4) {
    if (n1 < 0 || n1 > 10 || n2 < 0 || n2 > 10 || 
        n3 < 0 || n3 > 10 || n4 < 0 || n4 > 10) {
        return "Entrada invalida";
    }
    float media = (n1 + n2 + n3 + n4) / 4.0;
    if (media >= 7) return "Aprovado";
    else if (media >= 4) return "Final";
    else return "Reprovado";
}

int main() {
    float n1, n2, n3, n4;
    printf("Digite as 4 notas: ");
    scanf("%f %f %f %f", &n1, &n2, &n3, &n4);
    printf("Situacao: %s\n", situacaoAluno(n1, n2, n3, n4));
    return 0;
}
